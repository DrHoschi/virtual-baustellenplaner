{
  "pluginId": "mod.assets.local",
  "version": "1.0.0",
  "ui": {
    "menuEntries": [
      {
        "anchor": "projectPanel",
        "tabId": "assets",
        "title": "Projekt-Assets",
        "icon": "upload",
        "order": 50,
        "requires": [
          "features.flags.assetLibraries"
        ]
      }
    ]
  },
  "settings": {
    "path": "modules.projectSettings.assets",
    "defaults": {
      "projectAssets": [],
      "import": {
        "defaultCategory": "misc",
        "autoTag": true
      },
      "bindings": {
        "preferLibraryAssets": true
      },
      "validation": {
        "checkMissing": true,
        "checkDuplicates": true
      }
    }
  },
  "meta": {
    "description": "Projekt-Uploads, Bindings, Platzhalter, Asset-Versionen.",
    "devNote": "DEV Maximalausbau: requires werden in appMode=dev ignoriert."
  }
}
